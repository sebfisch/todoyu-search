<ul>
{foreach $ type resultType}
	<li>
		<span class="type type{ucfirst($type)}">{$resultType.label}</span>
		<ul>
		{foreach $resultType.results result}
			<li>
				<a href="javascript:void(0)" onclick="{$result.onclick}" title="{$result.title}">
					<span class="title">{$result.labelTitle}</span>
					<span class="info">{$result.labelInfo}</span>
				</a>
			</li>
		{/foreach}
		</ul>
	</li>
{/foreach}
</ul>